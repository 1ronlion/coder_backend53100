{{#if isValid}}
<h2>Listado de alumnos</h2>
<hr>
<div>
    {{#each docs}}
    <div>
        <p><strong> Nombre:</strong> {{this.first_name}} &nbsp; <strong>Apellido:</strong> {{this.last_name}}</p>
        <p><strong>Grupo:</strong> {{this.group}} &nbsp; <strong>Calificaci√≥n:</strong> {{this.grade}}</p>
    </div>
    <br/>
    {{/each}}
</div>
<br/>
<hr>
<div>
    {{#if hasPrevPage}}
        <a href={{this.prevLink}}><< Anterior</a>
    {{/if}}
    &nbsp;
    {{this.page}}
    &nbsp;
     {{#if hasNextPage}}
        <a href={{this.nextLink}}>Siguiente >></a>
